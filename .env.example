# .env.example

# Ethereum private key for your wallet (DO NOT push the real private key to public repositories)
PRIVATE_KEY=your_private_key_here

# Infura API Key (or Alchemy, QuickNode, etc. for Ethereum network access)
INFURA_API_KEY=your_infura_api_key_here

# Ethereum network (mainnet, ropsten, rinkeby, etc.)
NETWORK=ropsten

# Gas settings (optional)
GAS_LIMIT=8000000
GAS_PRICE=20000000000  # Gwei

# Address of deployed contract (e.g., MXFToken contract)
GASX_CONTRACT_ADDRESS=your_contract_address_here

